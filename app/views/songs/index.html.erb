<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.release_year %></td>
        <td><%= s.genre %></td>
        <td><%= s.artist_name %></td>
      </tr>
      <tr>
        <%= link_to 'View', song_path(@song) %> |
        <%= link_to 'Edit', edit_song_path(song) %>
      </tr>
    <% end %>
  </tbody>
</table>